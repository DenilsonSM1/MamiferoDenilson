generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cachorro {
  id          Int      @id @default(autoincrement())
  name        String?  @db.VarChar(30)
  age         Int?
  description String?  @db.VarChar(100)
  vaccinated  Boolean?
}
